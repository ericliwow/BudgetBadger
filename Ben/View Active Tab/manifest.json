{
    "name": "View Active Tab",
    "description": "A Simple Extension to View Recently Active Tabs",
    "version": "0.1",
    "manifest_version": 2,
    "action": {
        "default_popup": "popup.html",
        "default_icon": "H2.jpg"
    },
    "permissions": [
        "history",
        "tabs"
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    }
}
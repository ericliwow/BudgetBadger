{
    "name": "Budget Badger",
    "description": "A Simple Extension to View Recently Active Tabs, Longest Viewed Domain, and a Timer",
    "version": "0.1",
    "manifest_version": 3,
    "action": {
        "default_popup": "popup.html",
        "default_icon": "H2.jpg"
    },
    "permissions": [
        "history",
        "tabs",
        "storage"
    ],
    "background": {
        "service_worker": "background.js"
    }
}